import pygame
pygame.init()
SH,SW=500,500
screen=pygame.display.set_mode((SH,SW))
pygame.display.set_caption("my first game")
background_image = pygame.transf
done=False
while not done:
    for event in pygame.event.get():
        if event.type==pygame.QUIT:
            pygame.quit()
    pygame.display.flip()





